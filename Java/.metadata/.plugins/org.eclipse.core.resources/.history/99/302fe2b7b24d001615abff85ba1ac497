    }
}
class Account{
	float amount;
	
	public void WithDraw(Float t){
		this.amount+=t;
	}
	
	public void Deposit(float t) {
		this.amount-=t;
	}
	
	public float GetBlance()
	{
		return amount;
	}
	
}